# $Id:$

# calc prime

defun primea (n) {
    set prime [vector (2 3 5)];
    set cand 5;
    set c [$prime len];

    while {true} do: {
	if {[$prime last] > $n} then: {return $prime};

	set cand [$cand + 2];
	$prime each do: {| i |
	    if {[$i * 2] > $cand} then: {break};
	    if {[$cand % $i] = 0} then: {set cand [$cand + 2]; retry};
	};

        #found prime
	$prime + $cand;
	$c ++;
    };
};
