# $Id:$

# calc prime

defun prime (n) {
  set prime (3 5);
  set last [$prime last];
  set cand [$last item];
  set c [$prime len];

  while {true} do: {
    if {[$last item] > $n} then: {return $prime};


    $cand ++ 2;
    $prime each do: {| i |
      if {[$i * 2] > $cand} then: {break};
      if {[$cand % $i] = 0} then: {$cand ++ 2; retry};
    };
    #found prime
    set last [$last + $cand];
    set cand [$last item];
    $c ++;
  };
};
