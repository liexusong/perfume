# factorical calc with Y combinatore.
# [Y [fun (f) {fun (x) {if {$x <= 1} then: {return 1}; $x * [$f [$x - 1]]}}]] X

print file: $stdout "input feb number => ";
$stdout flush;
read x :nonewline;
println [
    [Y [
	fun (f) {
	    fun (x) {
	        if {$x <= 1} then: {return 1};
		$x * [$f [$x - 1]];
	    }
	}
    ]] [$x int];
];
